bido\n      product: this.getProductById(this.id) || {}\n    }\n  },\n  methods: {\n    goHome() {\n      this.$router.push('/');\n    },\n    getProductById(id) {\n      // Implementa la lógica para obtener los detalles del producto usando el id\n      // Ejemplo: retorno un producto mock\n      const products = [\n        { id: 1, nombre: \"Desayuno\", descripcion: \"Blalala\", precio: 5000, cantidad: 1 },\n        { id: 2, nombre: \"Almuerzo\", descripcion: \"Comida deliciosa\", precio: 8000, cantidad: 2 },\n        // otros productos...\n      ];\n      return products.find(product => product.id === id);\n    }\n  }\n};\n</script>\n\n<style scoped>\nh1 {\n  text-align: center;\n  margin-top: 120px;\n}\n\n.home-button {\n  display: block;\n  margin: 20px auto;\n  padding: 10px 20px;\n  font-size: 16px;\n  cursor: pointer;\n  border-radius: 5px;\n}\n\n.home-button:hover {\n  background-color: #ff96c5;\n}\n</style>\n\n\n"],"mappings":";AAaA,OAAOA,IAAG,MAAO,wBAAwB;AAEzC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,EAAE,EAAEC,MAAK,CAAE;EACb,CAAC;;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAC,OAAO,EAAE,IAAI,CAACC,cAAc,CAAC,IAAI,CAACJ,EAAE,KAAK,CAAC;IAC5C;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACP,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;IACxB,CAAC;IACDJ,cAAcA,CAACJ,EAAE,EAAE;MACjB;MACA;MACA,MAAMS,QAAO,GAAI,CACf;QAAET,EAAE,EAAE,CAAC;QAAEU,MAAM,EAAE,UAAU;QAAEC,WAAW,EAAE,SAAS;QAAEC,MAAM,EAAE,IAAI;QAAEC,QAAQ,EAAE;MAAE,CAAC,EAChF;QAAEb,EAAE,EAAE,CAAC;QAAEU,MAAM,EAAE,UAAU;QAAEC,WAAW,EAAE,kBAAkB;QAAEC,MAAM,EAAE,IAAI;QAAEC,QAAQ,EAAE;MAAE;MACxF;MAAA,CACD;;MACD,OAAOJ,QAAQ,CAACK,IAAI,CAACX,OAAM,IAAKA,OAAO,CAACH,EAAC,KAAMA,EAAE,CAAC;IACpD;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}